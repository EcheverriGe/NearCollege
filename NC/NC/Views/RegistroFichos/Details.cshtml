@model NC.Models.Tbl_Fichos

@{
    ViewBag.Title = "Detalles ficho";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h2 class="text-center">Detalles ficho</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label>Fecha:</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaFicho)
        </dd>

        <dt>
            <label>Hora:</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HoraFicho)
        </dd>

        <dt>
            <label>Usuario:</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdUsuario)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-info" })
</p>
